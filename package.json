{
  "name": "@mvtcode/vue3-querybuilder",
  "version": "0.1.1",
  "description": "A powerful and flexible query builder component for Vue 3 with Element Plus",
  "keywords": [
    "vue",
    "vue3",
    "query-builder",
    "element-plus",
    "typescript"
  ],
  "author": "tanmv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:mvtcode/vue3-querybuilder.git"
  },
  "private": false,
  "type": "module",
  "main": "dist/vue3-querybuilder.umd.js",
  "module": "dist/vue3-querybuilder.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vue3-querybuilder.es.js",
      "require": "./dist/vue3-querybuilder.umd.js"
    },
    "./dist/style.css": "./dist/vue3-querybuilder.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --build && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.6.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "prettier": "3.5.3",
    "sass-embedded": "^1.86.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
